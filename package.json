{
	"name": "ayfri-minecraft-game",
	"version": "0.1.0",
	"description": "A Minecraft like made with p5.js in TypeScript.",
	"main": "index.js",
	"scripts": {
		"start": "webpack-dev-server --port 9000 --inline --progress --profile --colors --open --watch --content-base src/ --mode development",
		"git": "git rm -rf dist --ignore-unmatch && npm run build && git add *",
		"build": "webpack --config webpack.config.js --mode production",
		"build.prod": "webpack --config webpack.config.js -p",
		"build.esnext": "npm run build:types && npm run build:js",
		"build:types": "tsc --emitDeclarationOnly",
		"build:js": "babel src --out-dir dist --extensions \".ts,.tsx\" --source-maps inline",
		"build.all": "npm run build.prod && npm run build.esnext",
		"test": "karma start karma.config.js"
	},
	"keywords": [
		"webpack",
		"webpack4",
		"typescript"
	],
	"author": "Ayfri",
	"license": "ISC",
	"devDependencies": {
		"@babel/cli": "^7.12.7",
		"@babel/core": "^7.12.7",
		"@babel/plugin-proposal-class-properties": "^7.12.1",
		"@babel/plugin-proposal-object-rest-spread": "^7.12.1",
		"@babel/preset-env": "^7.12.13",
		"@babel/preset-typescript": "^7.12.7",
		"@types/grecaptcha": "^3.0.1",
		"@types/jasmine": "3.6.2",
		"@types/node": "14.14.3",
		"@types/p5": "^0.9.1",
		"awesome-typescript-loader": "5.2.1",
		"css-loader": "^5.0.0",
		"file-loader": "^6.2.0",
		"html-loader": "^1.3.2",
		"html-webpack-plugin": "^4.5.0",
		"jasmine-core": "3.6.0",
		"karma": "5.2.3",
		"karma-jasmine": "4.0.1",
		"karma-phantomjs-launcher": "1.0.4",
		"karma-webpack": "4.0.2",
		"source-map-loader": "1.1.3",
		"style-loader": "^2.0.0",
		"tslint": "6.1.3",
		"tslint-loader": "3.6.0",
		"typescript": "3.9.7",
		"webpack": "^4.44.2",
		"webpack-cli": "4.2.0",
		"webpack-dev-server": "^3.11.0"
	},
	"dependencies": {
		"p5": "^1.2.0"
	}
}
